{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:base"
	],
	"packageRules": [
		{
			"matchPackagePatterns": [
				"*"
			],
			"matchUpdateTypes": [
				"minor",
				"patch"
			],
			"groupName": "all-non-major",
			"groupSlug": "all-non-major",
			"automerge": true
		}
	]
}